{
    "version": 2,
    "builds": [
        {
            "src": "api/search.js",
            "use": "@vercel/node"
        },
        {
            "src": "(index.html|search.html|app.js|data.js|questions.js|styles.css|*.png)",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/search",
            "dest": "api/search.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}